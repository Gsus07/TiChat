<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="rainbowGrad" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#F44336;stop-opacity:1" />
      <stop offset="16.66%" style="stop-color:#FF9800;stop-opacity:1" />
      <stop offset="33.33%" style="stop-color:#FFEB3B;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#4CAF50;stop-opacity:1" />
      <stop offset="66.66%" style="stop-color:#2196F3;stop-opacity:1" />
      <stop offset="83.33%" style="stop-color:#9C27B0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E91E63;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="targetGrad" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#FFD54F;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FF5722;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D32F2F;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Colorful background -->
  <rect width="400" height="400" fill="#E8F5E8"/>
  
  <!-- Rainbow track for racing -->
  <rect x="0" y="180" width="400" height="40" fill="url(#rainbowGrad)" opacity="0.7"/>
  
  <!-- Parkour platforms -->
  <rect x="50" y="300" width="40" height="20" fill="#4CAF50" stroke="#2E7D32" stroke-width="2"/>
  <rect x="120" y="280" width="40" height="20" fill="#2196F3" stroke="#0D47A1" stroke-width="2"/>
  <rect x="190" y="260" width="40" height="20" fill="#FF9800" stroke="#E65100" stroke-width="2"/>
  <rect x="260" y="240" width="40" height="20" fill="#E91E63" stroke="#AD1457" stroke-width="2"/>
  <rect x="330" y="220" width="40" height="20" fill="#9C27B0" stroke="#6A1B9A" stroke-width="2"/>
  
  <!-- Target for archery minigame -->
  <circle cx="100" cy="120" r="40" fill="url(#targetGrad)"/>
  <circle cx="100" cy="120" r="30" fill="white"/>
  <circle cx="100" cy="120" r="20" fill="url(#targetGrad)"/>
  <circle cx="100" cy="120" r="10" fill="white"/>
  <circle cx="100" cy="120" r="5" fill="#D32F2F"/>
  
  <!-- Arrow hitting target -->
  <rect x="95" y="115" width="20" height="3" fill="#8D6E63"/>
  <polygon points="115,115 125,118 115,121" fill="#616161"/>
  <rect x="90" y="116" width="8" height="1" fill="#FF5722"/>
  
  <!-- Spleef arena -->
  <rect x="250" y="100" width="120" height="80" fill="white" stroke="#E0E0E0" stroke-width="2"/>
  <!-- Missing blocks in spleef arena -->
  <rect x="270" y="120" width="20" height="20" fill="#E8F5E8"/>
  <rect x="310" y="140" width="20" height="20" fill="#E8F5E8"/>
  <rect x="330" y="120" width="20" height="20" fill="#E8F5E8"/>
  
  <!-- Player figures -->
  <!-- Player 1 (red) -->
  <rect x="280" y="90" width="8" height="12" fill="#F44336"/>
  <rect x="282" y="85" width="4" height="4" fill="#FFDBCB"/>
  <rect x="278" y="102" width="4" height="8" fill="#1976D2"/>
  <rect x="286" y="102" width="4" height="8" fill="#1976D2"/>
  
  <!-- Player 2 (blue) -->
  <rect x="320" y="90" width="8" height="12" fill="#2196F3"/>
  <rect x="322" y="85" width="4" height="4" fill="#FFDBCB"/>
  <rect x="318" y="102" width="4" height="8" fill="#424242"/>
  <rect x="326" y="102" width="4" height="8" fill="#424242"/>
  
  <!-- TNT blocks for TNT run -->
  <rect x="30" y="250" width="20" height="20" fill="#F44336" stroke="#D32F2F" stroke-width="2"/>
  <text x="40" y="265" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white">TNT</text>
  
  <rect x="380" y="250" width="20" height="20" fill="#F44336" stroke="#D32F2F" stroke-width="2"/>
  <text x="390" y="265" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">TNT</text>
  
  <!-- Explosion effects -->
  <circle cx="40" cy="240" r="15" fill="#FF5722" opacity="0.6">
    <animate attributeName="r" values="5;25;5" dur="1s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.8;0.2;0.8" dur="1s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Capture the flag flags -->
  <rect x="20" y="50" width="3" height="30" fill="#8D6E63"/>
  <rect x="23" y="50" width="15" height="10" fill="#F44336"/>
  
  <rect x="377" y="50" width="3" height="30" fill="#8D6E63"/>
  <rect x="365" y="50" width="15" height="10" fill="#2196F3"/>
  
  <!-- Coins/points floating -->
  <circle cx="150" cy="50" r="8" fill="#FFD700" stroke="#FF8F00" stroke-width="2">
    <animate attributeName="cy" values="50;30;50" dur="2s" repeatCount="indefinite"/>
  </circle>
  <text x="150" y="55" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="#FF8F00">$</text>
  
  <circle cx="250" cy="40" r="6" fill="#FFD700" stroke="#FF8F00" stroke-width="2">
    <animate attributeName="cy" values="40;20;40" dur="1.5s" repeatCount="indefinite"/>
  </circle>
  <text x="250" y="44" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="#FF8F00">$</text>
  
  <!-- Leaderboard -->
  <rect x="300" y="300" width="80" height="60" fill="#37474F" stroke="#263238" stroke-width="2"/>
  <text x="340" y="315" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="#FFD700">LEADERBOARD</text>
  <text x="310" y="330" font-family="Arial, sans-serif" font-size="8" fill="#4CAF50">1. Player1 - 150</text>
  <text x="310" y="342" font-family="Arial, sans-serif" font-size="8" fill="#2196F3">2. Player2 - 120</text>
  <text x="310" y="354" font-family="Arial, sans-serif" font-size="8" fill="#FF9800">3. Player3 - 95</text>
  
  <!-- Game controller icon -->
  <rect x="180" y="320" width="40" height="25" fill="#424242" stroke="#212121" stroke-width="2" rx="5"/>
  <circle cx="190" cy="332" r="4" fill="#616161"/>
  <circle cx="210" cy="332" r="4" fill="#616161"/>
  <rect x="195" y="325" width="10" height="3" fill="#616161"/>
  <rect x="198" y="322" width="4" height="9" fill="#616161"/>
  
  <!-- Confetti particles -->
  <rect x="60" y="30" width="3" height="3" fill="#F44336" transform="rotate(45 61 31)">
    <animate attributeName="y" values="30;380" dur="3s" repeatCount="indefinite"/>
  </rect>
  <rect x="340" y="20" width="3" height="3" fill="#4CAF50" transform="rotate(30 341 21)">
    <animate attributeName="y" values="20;380" dur="2.5s" repeatCount="indefinite"/>
  </rect>
  <rect x="120" y="25" width="3" height="3" fill="#2196F3" transform="rotate(60 121 26)">
    <animate attributeName="y" values="25;380" dur="2.8s" repeatCount="indefinite"/>
  </rect>
  
  <!-- Title -->
  <text x="200" y="390" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="url(#rainbowGrad)">MINIGAMES</text>
</svg>
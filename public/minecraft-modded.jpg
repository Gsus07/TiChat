<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="techGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00BCD4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0097A7;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="machineGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#607D8B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#37474F;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="energyGrad" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#00E676;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00C853;stop-opacity:0.3" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="#263238"/>
  
  <!-- Ground -->
  <rect x="0" y="320" width="400" height="80" fill="#424242"/>
  
  <!-- Industrial machine -->
  <rect x="100" y="200" width="80" height="120" fill="url(#machineGrad)" stroke="#455A64" stroke-width="2"/>
  <rect x="110" y="210" width="60" height="20" fill="#00BCD4" stroke="#0097A7" stroke-width="1"/>
  <circle cx="140" cy="250" r="15" fill="url(#energyGrad)"/>
  
  <!-- Pipes -->
  <rect x="180" y="240" width="60" height="10" fill="#607D8B" stroke="#455A64" stroke-width="1"/>
  <rect x="235" y="200" width="10" height="50" fill="#607D8B" stroke="#455A64" stroke-width="1"/>
  
  <!-- Energy conduits -->
  <rect x="50" y="280" width="300" height="8" fill="url(#techGrad)" opacity="0.8"/>
  <rect x="50" y="290" width="300" height="4" fill="#00E676" opacity="0.6"/>
  
  <!-- Reactor core -->
  <rect x="250" y="180" width="60" height="140" fill="url(#machineGrad)" stroke="#455A64" stroke-width="2"/>
  <circle cx="280" cy="220" r="20" fill="url(#energyGrad)"/>
  <circle cx="280" cy="220" r="12" fill="#00E676" opacity="0.8"/>
  
  <!-- Energy particles -->
  <circle cx="120" cy="150" r="3" fill="#00E676" opacity="0.9">
    <animate attributeName="cy" values="150;100;150" dur="2s" repeatCount="indefinite"/>
  </circle>
  <circle cx="280" cy="140" r="4" fill="#00BCD4" opacity="0.8">
    <animate attributeName="cy" values="140;90;140" dur="1.5s" repeatCount="indefinite"/>
  </circle>
  <circle cx="200" cy="160" r="2" fill="#4CAF50" opacity="0.7">
    <animate attributeName="cy" values="160;110;160" dur="2.5s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Mod symbols -->
  <rect x="30" y="100" width="25" height="25" fill="#FF5722" stroke="#D84315" stroke-width="2" transform="rotate(45 42 112)"/>
  <polygon points="350,80 360,100 370,80 370,110 350,110" fill="#9C27B0" stroke="#6A1B9A" stroke-width="2"/>
  <circle cx="80" cy="80" r="12" fill="#FF9800" stroke="#E65100" stroke-width="2"/>
  
  <!-- Tech interface -->
  <rect x="320" y="100" width="60" height="40" fill="#37474F" stroke="#263238" stroke-width="2"/>
  <rect x="325" y="105" width="10" height="5" fill="#00E676"/>
  <rect x="340" y="105" width="10" height="5" fill="#FF5722"/>
  <rect x="355" y="105" width="10" height="5" fill="#2196F3"/>
  <rect x="325" y="115" width="50" height="3" fill="#00BCD4"/>
  <rect x="325" y="125" width="35" height="3" fill="#4CAF50"/>
  <rect x="325" y="135" width="40" height="3" fill="#FF9800"/>
  
  <!-- Holographic display -->
  <polygon points="150,50 200,30 250,50 200,70" fill="#00BCD4" opacity="0.3" stroke="#0097A7" stroke-width="1"/>
  <text x="200" y="55" text-anchor="middle" font-family="monospace" font-size="12" fill="#00E676">MOD v2.1</text>
  
  <!-- Title -->
  <text x="200" y="380" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#00BCD4">MODDED</text>
</svg>
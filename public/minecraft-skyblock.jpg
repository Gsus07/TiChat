<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#E3F2FD;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#BBDEFB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#90CAF9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="islandGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8D6E63;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#5D4037;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="grassGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7CB342;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#558B2F;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="400" height="400" fill="url(#skyGradient)"/>
  
  <!-- Clouds -->
  <ellipse cx="80" cy="60" rx="30" ry="15" fill="white" opacity="0.8"/>
  <ellipse cx="320" cy="80" rx="25" ry="12" fill="white" opacity="0.7"/>
  <ellipse cx="150" cy="40" rx="20" ry="10" fill="white" opacity="0.6"/>
  <ellipse cx="280" cy="50" rx="35" ry="18" fill="white" opacity="0.8"/>
  
  <!-- Main floating island -->
  <ellipse cx="200" cy="280" rx="80" ry="25" fill="url(#islandGrad)"/>
  <ellipse cx="200" cy="270" rx="75" ry="20" fill="url(#grassGrad)"/>
  
  <!-- Smaller floating islands -->
  <ellipse cx="100" cy="200" rx="30" ry="10" fill="url(#islandGrad)"/>
  <ellipse cx="100" cy="195" rx="28" ry="8" fill="url(#grassGrad)"/>
  
  <ellipse cx="320" cy="220" rx="25" ry="8" fill="url(#islandGrad)"/>
  <ellipse cx="320" cy="216" rx="23" ry="6" fill="url(#grassGrad)"/>
  
  <ellipse cx="80" cy="320" rx="20" ry="6" fill="url(#islandGrad)"/>
  <ellipse cx="80" cy="317" rx="18" ry="4" fill="url(#grassGrad)"/>
  
  <!-- Tree on main island -->
  <rect x="180" y="230" width="12" height="40" fill="#8D6E63" stroke="#5D4037" stroke-width="1"/>
  <circle cx="186" cy="225" r="18" fill="#4CAF50" stroke="#2E7D32" stroke-width="1"/>
  
  <!-- Small house on main island -->
  <rect x="210" y="240" width="30" height="30" fill="#8D6E63" stroke="#5D4037" stroke-width="1"/>
  <polygon points="210,240 225,225 240,240" fill="#D32F2F" stroke="#B71C1C" stroke-width="1"/>
  <rect x="218" y="255" width="6" height="12" fill="#3E2723"/>
  <rect x="228" y="248" width="6" height="6" fill="#81D4FA" stroke="#0277BD" stroke-width="0.5"/>
  
  <!-- Chest -->
  <rect x="160" y="260" width="15" height="10" fill="#8D6E63" stroke="#5D4037" stroke-width="1"/>
  <rect x="162" y="258" width="11" height="4" fill="#A1887F"/>
  
  <!-- Water bucket -->
  <rect x="250" y="265" width="8" height="8" fill="#607D8B" stroke="#455A64" stroke-width="1"/>
  <rect x="251" y="264" width="6" height="2" fill="#2196F3"/>
  
  <!-- Cobblestone generator setup -->
  <rect x="140" y="265" width="8" height="8" fill="#616161" stroke="#424242" stroke-width="1"/>
  <rect x="150" y="265" width="8" height="8" fill="#2196F3" opacity="0.7"/>
  <rect x="160" y="265" width="8" height="8" fill="#FF5722" opacity="0.7"/>
  
  <!-- Floating resources -->
  <rect x="90" y="180" width="6" height="6" fill="#616161" stroke="#424242" stroke-width="1"/>
  <rect x="310" y="200" width="5" height="5" fill="#8D6E63" stroke="#5D4037" stroke-width="1"/>
  
  <!-- Bridge between islands -->
  <rect x="120" y="198" width="60" height="4" fill="#8D6E63" stroke="#5D4037" stroke-width="1"/>
  
  <!-- Void indication (darker bottom) -->
  <rect x="0" y="350" width="400" height="50" fill="#000000" opacity="0.3"/>
  
  <!-- Falling blocks animation -->
  <rect x="50" y="300" width="4" height="4" fill="#8D6E63" opacity="0.6">
    <animate attributeName="y" values="300;400" dur="3s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.6;0" dur="3s" repeatCount="indefinite"/>
  </rect>
  <rect x="350" y="280" width="3" height="3" fill="#616161" opacity="0.5">
    <animate attributeName="y" values="280;400" dur="4s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.5;0" dur="4s" repeatCount="indefinite"/>
  </rect>
  
  <!-- Sun -->
  <circle cx="350" cy="50" r="25" fill="#FFD54F" stroke="#FF8F00" stroke-width="2"/>
  
  <!-- Achievement progress indicator -->
  <rect x="20" y="20" width="100" height="15" fill="#424242" stroke="#212121" stroke-width="1"/>
  <rect x="22" y="22" width="60" height="11" fill="#4CAF50"/>
  <text x="70" y="31" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="white">60%</text>
  
  <!-- Title -->
  <text x="200" y="380" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#1976D2">SKYBLOCK</text>
</svg>
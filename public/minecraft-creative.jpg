<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="skyGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#E3F2FD;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#BBDEFB;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="castleGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#BDBDBD;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#757575;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="400" height="400" fill="url(#skyGrad)"/>
  
  <!-- Floating islands -->
  <ellipse cx="80" cy="320" rx="60" ry="20" fill="#4CAF50"/>
  <ellipse cx="320" cy="280" rx="50" ry="15" fill="#4CAF50"/>
  <ellipse cx="200" cy="350" rx="40" ry="12" fill="#4CAF50"/>
  
  <!-- Castle structure -->
  <rect x="150" y="150" width="100" height="120" fill="url(#castleGrad)" stroke="#424242" stroke-width="2"/>
  <rect x="130" y="120" width="40" height="150" fill="url(#castleGrad)" stroke="#424242" stroke-width="2"/>
  <rect x="230" y="120" width="40" height="150" fill="url(#castleGrad)" stroke="#424242" stroke-width="2"/>
  
  <!-- Castle tops -->
  <polygon points="130,120 150,100 150,120" fill="#D32F2F"/>
  <polygon points="230,120 250,100 270,120" fill="#D32F2F"/>
  <polygon points="150,150 200,130 250,150" fill="#D32F2F"/>
  
  <!-- Windows -->
  <rect x="165" y="180" width="15" height="20" fill="#FFD54F"/>
  <rect x="220" y="180" width="15" height="20" fill="#FFD54F"/>
  <rect x="185" y="200" width="30" height="40" fill="#3E2723"/>
  
  <!-- Rainbow -->
  <path d="M 50 200 Q 200 100 350 200" stroke="#F44336" stroke-width="8" fill="none" opacity="0.7"/>
  <path d="M 50 210 Q 200 110 350 210" stroke="#FF9800" stroke-width="8" fill="none" opacity="0.7"/>
  <path d="M 50 220 Q 200 120 350 220" stroke="#FFEB3B" stroke-width="8" fill="none" opacity="0.7"/>
  <path d="M 50 230 Q 200 130 350 230" stroke="#4CAF50" stroke-width="8" fill="none" opacity="0.7"/>
  <path d="M 50 240 Q 200 140 350 240" stroke="#2196F3" stroke-width="8" fill="none" opacity="0.7"/>
  <path d="M 50 250 Q 200 150 350 250" stroke="#9C27B0" stroke-width="8" fill="none" opacity="0.7"/>
  
  <!-- Flying blocks -->
  <rect x="50" y="100" width="20" height="20" fill="#FFD54F" stroke="#FF8F00" stroke-width="1" transform="rotate(15 60 110)"/>
  <rect x="320" y="120" width="25" height="25" fill="#E91E63" stroke="#AD1457" stroke-width="1" transform="rotate(-20 332 132)"/>
  <rect x="100" y="80" width="18" height="18" fill="#2196F3" stroke="#0D47A1" stroke-width="1" transform="rotate(30 109 89)"/>
  
  <!-- Sparkles -->
  <polygon points="300,60 305,70 315,65 305,75 300,85 295,75 285,65 295,70" fill="#FFD700" opacity="0.8"/>
  <polygon points="120,50 123,57 130,55 123,62 120,70 117,62 110,55 117,57" fill="#FFD700" opacity="0.8"/>
  <polygon points="350,150 353,157 360,155 353,162 350,170 347,162 340,155 347,157" fill="#FFD700" opacity="0.8"/>
  
  <!-- Title -->
  <text x="200" y="50" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#1976D2">CREATIVE</text>
</svg>
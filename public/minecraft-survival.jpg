<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="grassGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7CB342;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#558B2F;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="dirtGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8D6E63;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#5D4037;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="#87CEEB"/>
  
  <!-- Ground blocks -->
  <rect x="0" y="300" width="80" height="80" fill="url(#grassGrad)" stroke="#4CAF50" stroke-width="2"/>
  <rect x="80" y="300" width="80" height="80" fill="url(#grassGrad)" stroke="#4CAF50" stroke-width="2"/>
  <rect x="160" y="300" width="80" height="80" fill="url(#grassGrad)" stroke="#4CAF50" stroke-width="2"/>
  <rect x="240" y="300" width="80" height="80" fill="url(#grassGrad)" stroke="#4CAF50" stroke-width="2"/>
  <rect x="320" y="300" width="80" height="80" fill="url(#grassGrad)" stroke="#4CAF50" stroke-width="2"/>
  
  <!-- Dirt layer -->
  <rect x="0" y="380" width="400" height="20" fill="url(#dirtGrad)"/>
  
  <!-- Tree -->
  <rect x="120" y="220" width="40" height="80" fill="#8D6E63" stroke="#5D4037" stroke-width="2"/>
  <circle cx="140" cy="200" r="40" fill="#4CAF50" stroke="#2E7D32" stroke-width="2"/>
  
  <!-- House -->
  <rect x="250" y="200" width="100" height="100" fill="#8D6E63" stroke="#5D4037" stroke-width="2"/>
  <polygon points="250,200 300,150 350,200" fill="#D32F2F" stroke="#B71C1C" stroke-width="2"/>
  <rect x="270" y="240" width="20" height="40" fill="#3E2723"/>
  <rect x="310" y="220" width="20" height="20" fill="#81D4FA" stroke="#0277BD" stroke-width="1"/>
  
  <!-- Sun -->
  <circle cx="350" cy="50" r="30" fill="#FFD54F" stroke="#FF8F00" stroke-width="2"/>
  
  <!-- Clouds -->
  <ellipse cx="100" cy="80" rx="40" ry="20" fill="white" opacity="0.8"/>
  <ellipse cx="300" cy="100" rx="35" ry="18" fill="white" opacity="0.8"/>
  
  <!-- Title -->
  <text x="200" y="50" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2E7D32">SURVIVAL</text>
</svg>